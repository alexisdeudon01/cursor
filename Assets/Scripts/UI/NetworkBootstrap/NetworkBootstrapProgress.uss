.bootstrap-root {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0, 0, 0, 0.65);
    justify-content: center;
    align-items: center;
    color: rgb(255, 255, 255);
}

.bootstrap-panel {
    width: 520px;
    max-width: 640px;
    padding-left: 20px;
    padding-right: 20px;
    padding-top: 16px;
    padding-bottom: 16px;
    background-color: rgba(31, 31, 31, 0.95);
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
    border-bottom-left-radius: 8px;
    border-bottom-right-radius: 8px;
    color: rgb(255, 255, 255);
}

.bootstrap-title {
    -unity-font-style: bold;
    font-size: 18px;
    margin-bottom: 8px;
}

.bootstrap-progress {
    height: 16px;
    margin-bottom: 8px;
}

.bootstrap-step {
    -unity-font-style: bold;
    margin-bottom: 4px;
}

.bootstrap-substep {
    margin-bottom: 6px;
}

.bootstrap-error {
    color: rgb(255, 102, 102);
    margin-bottom: 6px;
    display: none;
}

.bootstrap-log {
    height: 140px;
    margin-top: 6px;
}
